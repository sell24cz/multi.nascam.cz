    <script>
    $(document).ready(function() {
        $('.camera').click(function() {

            var userid = $(this).data('id');

            // AJAX request
            $.ajax({
                url: 'getValue/camera.php',
                type: 'post',
                data: {
                    userid: userid
                },
                success: function(response) {
                    // Add response in Modal body
                    $('.cam').html(response);

                    // Display Modal
                    $('#empModal').modal('show');
                }
            });
        });
    });
    </script>

